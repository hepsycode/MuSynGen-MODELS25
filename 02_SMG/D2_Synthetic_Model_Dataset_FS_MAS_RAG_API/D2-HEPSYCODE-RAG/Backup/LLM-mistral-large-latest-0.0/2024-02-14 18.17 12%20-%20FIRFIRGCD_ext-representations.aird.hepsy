<?xml version="1.0" encoding="UTF-8"?>
<hml:BehaviorSpecification xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hml="org.univaq.hepsy">
  <nodes xsi:type="hml:Stimulus" name="Stimulus">
    <ports name="stim_fir1_out_port">
      <pChannels name="stim_fir1_channel" pFrom="//@nodes.0/@ports.0" pTo="//@nodes.2/@processes.0/@ports.0"/>
    </ports>
  </nodes>
  <nodes xsi:type="hml:StructuredNode" name="FIRFIRGCD_ext">
    <nChannels name="stim_fir1_channel" nFrom="//@nodes.0" nTo="//@nodes.2/@processes.0" queueSize="8" rendezVous="true">
      <message name="stim_fir1_payload">
        <entry name="raw_signal" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="fir1_fir2_channel" nFrom="//@nodes.2/@processes.0" nTo="//@nodes.2/@processes.1" queueSize="8" rendezVous="true">
      <message name="fir1_fir2_payload">
        <entry name="filtered_signal" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="fir2_firext_channel" nFrom="//@nodes.2/@processes.1" nTo="//@nodes.2/@processes.2" queueSize="8" rendezVous="true">
      <message name="fir2_firext_payload">
        <entry name="refined_signal" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="firext_gcd_channel" nFrom="//@nodes.2/@processes.2" nTo="//@nodes.2/@processes.3" queueSize="8" rendezVous="true">
      <message name="firext_gcd_payload">
        <entry name="enhanced_signal" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="gcd_display_channel" nFrom="//@nodes.2/@processes.3" nTo="//@nodes.1" queueSize="8" rendezVous="true">
      <message name="gcd_display_payload">
        <entry name="gcd_result" type="sc_int"/>
      </message>
    </nChannels>
    <ports name="fir1_in_port" portExtension="//@nodes.2/@processes.0"/>
    <ports name="fir2_in_port" portExtension="//@nodes.2/@processes.1"/>
    <ports name="firext_in_port" portExtension="//@nodes.2/@processes.2"/>
    <ports name="gcd_in_port" portExtension="//@nodes.2/@processes.3"/>
    <processes name="fir1"/>
    <processes name="fir2"/>
    <processes name="fir_ext"/>
    <processes name="gcd"/>
  </nodes>
  <nodes xsi:type="hml:Display" name="Display">
    <ports name="system_display_in_port">
      <pChannels name="gcd_display_channel" pFrom="//@nodes.2/@processes.3/@ports.0" pTo="//@nodes.1/@ports.0"/>
    </ports>
  </nodes>
</hml:BehaviorSpecification>