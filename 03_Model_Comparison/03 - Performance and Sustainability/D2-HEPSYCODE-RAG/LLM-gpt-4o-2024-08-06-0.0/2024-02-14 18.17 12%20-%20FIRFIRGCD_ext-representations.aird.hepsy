<?xml version="1.0" encoding="UTF-8"?>
<hml:BehaviorSpecification xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hml="org.univaq.hepsy">
  <nodes xsi:type="hml:Stimulus" name="stimulus">
    <ports name="stim_fir1_out_port">
      <pChannels name="stim_fir1_channel" pFrom="//@nodes.0/@ports.0" pTo="//@nodes.1/@ports.0" queueSize="10" rendezVous="true">
        <message name="stim_fir1_payload">
          <entry name="rawData" type="sc_int"/>
        </message>
      </pChannels>
    </ports>
  </nodes>
  <nodes xsi:type="hml:StructuredNode" name="FIRFIRGCD_ext">
    <ports name="fir1_in_port" portExtension="//@nodes.1/@processes.0"/>
    <ports name="fir2_in_port" portExtension="//@nodes.1/@processes.1"/>
    <ports name="fir_ext_in_port" portExtension="//@nodes.1/@processes.2"/>
    <ports name="gcd_in_port" portExtension="//@nodes.1/@processes.3"/>
    <nChannels name="fir1_fir2_channel" nFrom="//@nodes.1/@processes.0" nTo="//@nodes.1/@processes.1" queueSize="10" rendezVous="true">
      <message name="fir1_fir2_payload">
        <entry name="filteredData" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="fir2_firext_channel" nFrom="//@nodes.1/@processes.1" nTo="//@nodes.1/@processes.2" queueSize="10" rendezVous="true">
      <message name="fir2_firext_payload">
        <entry name="refinedData" type="sc_int"/>
      </message>
    </nChannels>
    <nChannels name="firext_gcd_channel" nFrom="//@nodes.1/@processes.2" nTo="//@nodes.1/@processes.3" queueSize="10" rendezVous="true">
      <message name="firext_gcd_payload">
        <entry name="enhancedData" type="sc_int"/>
      </message>
    </nChannels>
    <processes name="fir1"/>
    <processes name="fir2"/>
    <processes name="fir_ext"/>
    <processes name="gcd"/>
  </nodes>
  <nodes xsi:type="hml:Display" name="display">
    <ports name="system_display_in_port">
      <pChannels name="gcd_display_channel" pFrom="//@nodes.1/@ports.3" pTo="//@nodes.2/@ports.0" queueSize="10" rendezVous="true">
        <message name="gcd_display_payload">
          <entry name="gcdResult" type="sc_int"/>
        </message>
      </pChannels>
    </ports>
  </nodes>
</hml:BehaviorSpecification>